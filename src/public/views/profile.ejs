<%- layout('layouts/main') %>

<div class="container">
    <div class="row">
        <div class="col-md-12 text-center">
            <div class="card">
                <div class="card-header">
                    <h2>Data User</h2>
                </div>
                <div class="card-body">
                    <ul class="list-group">
                        <li class="list-group-item">
                            <b>Email:</b>
                            <%= userData.email %>
                        </li>
                        <li class="list-group-item">
                            <b>Password:</b>
                            <%= userData.password %>
                        </li>
                        <li class="list-group-item">
                            <b>Id:</b>
                            <%= userData.id %>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>